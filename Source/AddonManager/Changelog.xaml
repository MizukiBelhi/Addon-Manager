<Window x:Class="AddonManager.Changelog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AddonManager"
        mc:Ignorable="d" Width="798.454" Height="821.849"
        Title="Changelog" Background="Black" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">

    <Canvas x:Name="ChangelogCanvas" HorizontalAlignment="Left" Height="326" VerticalAlignment="Top" Width="564">
        <Rectangle Height="326" Style="{DynamicResource AddonStyle}" Canvas.Left="0" Canvas.Top="0" Width="564"
                   RadiusX="5" RadiusY="5">
            <Rectangle.Effect>
                <DropShadowEffect />
            </Rectangle.Effect>
        </Rectangle>
        <WebBrowser x:Name="changelogBrowser" Height="791" Width="784"/>
    </Canvas>

</Window>